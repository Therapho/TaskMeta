@if(Users != null)
{
  <FluentStack Orientation="Orientation.Horizontal">
    @foreach (var user in Users)
    {
        <FluentButton class="selector" Appearance="@(user==selectedUser?Appearance.Accent:Appearance.Neutral)" @onclick="(()=>HandleButtonClick(user))" >@user.UserName</FluentButton>
    }
    </FluentStack>
}